    <script>
        // Add basic interactivity for calendar dates
        document.querySelectorAll('button[class*="flex flex-col items-center w-12 h-12"]').forEach(button => {
            button.addEventListener('click', function() {
                // Remove selected state from all dates
                document.querySelectorAll('.bg-teal-500').forEach(el => {
                    el.classList.remove('bg-teal-500', 'text-white');
                    el.classList.add('hover:bg-teal-100');
                });

                // Add selected state to clicked date
                const dateDiv = this.querySelector('div');
                dateDiv.classList.add('bg-teal-500', 'text-white');
                dateDiv.classList.remove('hover:bg-teal-100');
            });
        });

        // Add interactivity for time slots
        document.querySelectorAll('button[class*="border border-gray-300"]').forEach(button => {
            button.addEventListener('click', function() {
                // Remove selected state from all time slots
                document.querySelectorAll('.bg-teal-500').forEach(el => {
                    if (el.parentElement.classList.contains('border-gray-300')) {
                        el.parentElement.classList.remove('bg-teal-500', 'border-teal-500');
                        el.parentElement.classList.add('bg-slate-50', 'border-gray-300');
                        el.classList.remove('text-white');
                        el.classList.add('text-teal-700');
                    }
                });

                // Add selected state to clicked time slot
                this.classList.remove('bg-slate-50', 'border-gray-300');
                this.classList.add('bg-teal-500', 'border-teal-500');
                const span = this.querySelector('span');
                span.classList.remove('text-teal-700');
                span.classList.add('text-white');
            });
        });
    </script>

